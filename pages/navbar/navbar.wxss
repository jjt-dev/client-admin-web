/* components/navbar/index.wxss */

.navbar {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  flex-shrink: 0;
}

.backimg{
  width:750rpx;
  height:160rpx;
  position: relative;
}
.backimg .background{
  width: 100%;
  height: 100%;
  position: absolute;
  z-index: 2;
}
.backimg .back{
  width:20rpx;
  height:35rpx;
  position: absolute;
  left: 52rpx;
  bottom: 53rpx;
  z-index:10;
}
/* title */
.navbar-title{
  font-size:15px;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(255,255,255,1);
  text-align: center;
  position: absolute;
  z-index: 3;
  bottom: 53rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
}

