@import "../../pages/resultsNo/resultsNo.wxss";
.body{
  background: #F0EDF1;
}
.listcontent{
  width:679rpx;
  height:120rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 6rpx 0px rgba(28,44,29,0.13);
  border-radius:10rpx;
  margin: 38rpx auto 0;
  display: flex;
  position: relative;

}
.listcontent .icon{
  width:82rpx;
  height:82rpx;
  border-radius:50%;
  margin-left: 29rpx;
  margin-top: 19rpx;
}
.listcontent label{
  font-size:15px;
  font-family:Microsoft YaHei;
  font-weight:400;
  color:rgba(67,67,67,1);
  height: 100%;
  line-height: 120rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: block;
  margin-left: 34rpx;
}
.listcontent .right{
  width:20rpx;
  height:35rpx;
  position: absolute;
  right: 15rpx;
  top: 45rpx;
}
